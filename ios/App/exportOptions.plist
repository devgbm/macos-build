<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Tipo de distribuição -->
    <key>method</key>
    <string>app-store</string>
    
    <!-- Seu Team ID (10 caracteres) -->
    <key>teamID</key>
    <string>ABC123DEFG</string>
    
    <!-- NÃO enviar bitcode (descontinuado pela Apple) -->
    <key>uploadBitcode</key>
    <false/>
    
    <!-- SIM enviar símbolos de debug (para crash reports) -->
    <key>uploadSymbols</key>
    <true/>
    
    <!-- Assinatura MANUAL (você controla tudo) -->
    <key>signingStyle</key>
    <string>manual</string>
    
    <!-- Mapeamento: qual profile usar para cada Bundle ID -->
    <key>provisioningProfiles</key>
    <dict>
        <key>com.meuapp.principal</key>
        <string>Meu App Store Profile</string>
    </dict>
</dict>
</plist>